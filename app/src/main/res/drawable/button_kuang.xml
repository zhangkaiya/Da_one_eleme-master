<?xml version="1.0" encoding="utf-8"?>
<shape xmlns:android="http://schemas.android.com/apk/res/android">
    <solid android:color="#00ffffff" />
    <!-- 背景填充颜色 -->
    <stroke
        android:width="1dp"
        android:color="#111dc6" />
    <!-- 描边，边框宽度、颜色 -->
    <corners android:radius="10dp" />
    <!-- 边角圆弧的半径 -->
    <padding
        android:bottom="3dp"
        android:left="3dp"
        android:right="3dp"
        android:top="3dp" />
    <!-- 四周留出来的空白 -->
</shape>
